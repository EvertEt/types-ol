{
  "$schema": "http://json.schemastore.org/prettierrc",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": ["*.ts"],
      "options": {
        "parser": "typescript",
        "tabWidth": 4
      }
    }
  ],
  "printWidth": 120,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrderSeparation": true
}
