{
  "opts": {
    "recurse": true,
    "template": "jsdoc/template"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": ["openlayers/src/ol"]
  },
  "plugins": [
    "jsdoc/plugins/markdown",
    "jsdoc-plugin-typescript",
    "jsdoc/plugins/inline-options",
    "jsdoc/plugins/inheritdoc",
    "jsdoc/plugins/events",
    "jsdoc/plugins/observable",
    "jsdoc/plugins/api",
    "jsdoc/plugins/exports",
    "jsdoc/plugins/template"
  ],
  "typescript": {
    "moduleRoot": "openlayers/src",
    "definition": {
      "extraOptions": true,
      "mode": "multiple",
      "strictGenericTypes": false,
      "strictReturnTypes": false
    }
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    },
    "applicationName": "OpenLayers"
  },
  "jsVersion": 180
}
